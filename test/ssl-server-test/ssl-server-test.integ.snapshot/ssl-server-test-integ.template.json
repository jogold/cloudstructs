        },
        "DefinitionString": {
          "Fn::Join": [
            "",
            [
              "{\"StartAt\":\"Start Analysis\",\"States\":{\"Start Analysis\":{\"Next\":\"Wait\",\"Retry\":[{\"ErrorEquals\":[\"Lambda.ServiceException\",\"Lambda.AWSLambdaException\",\"Lambda.SdkClientException\"],\"IntervalSeconds\":2,\"MaxAttempts\":6,\"BackoffRate\":2}],\"Type\":\"Task\",\"Resource\":\"",
              {
                "Fn::GetAtt": [
                  "SslServerTestAnalyzeFunction376D3549",
                  "Arn"
                ]
              },
              "\",\"Parameters\":{\"host.$\":\"$.host\",\"startNew\":\"on\"}},\"Wait\":{\"Type\":\"Wait\",\"Seconds\":10,\"Next\":\"Poll Analysis\"},\"Poll Analysis\":{\"Next\":\"Is Ready ?\",\"Retry\":[{\"ErrorEquals\":[\"Lambda.ServiceException\",\"Lambda.AWSLambdaException\",\"Lambda.SdkClientException\"],\"IntervalSeconds\":2,\"MaxAttempts\":6,\"BackoffRate\":2}],\"Type\":\"Task\",\"Resource\":\"",
              {
                "Fn::GetAtt": [
                  "SslServerTestAnalyzeFunction376D3549",
                  "Arn"
                ]
              },
              "\",\"Parameters\":{\"host.$\":\"$.host\"}},\"Is Ready ?\":{\"Type\":\"Choice\",\"Choices\":[{\"Variable\":\"$.status\",\"StringEquals\":\"READY\",\"Next\":\"Extract Grade\"},{\"Variable\":\"$.status\",\"StringEquals\":\"ERROR\",\"Next\":\"Fail\"}],\"Default\":\"Wait\"},\"Extract Grade\":{\"Next\":\"Is Grade Below Minimum?\",\"Retry\":[{\"ErrorEquals\":[\"Lambda.ServiceException\",\"Lambda.AWSLambdaException\",\"Lambda.SdkClientException\"],\"IntervalSeconds\":2,\"MaxAttempts\":6,\"BackoffRate\":2}],\"Type\":\"Task\",\"ResultPath\":\"$.grade\",\"Resource\":\"",
              {
                "Fn::GetAtt": [
                  "SslServerTestExtractGradeFunction39E961E5",
                  "Arn"
                ]
              },
              "\"},\"Is Grade Below Minimum?\":{\"Type\":\"Choice\",\"Choices\":[{\"Variable\":\"$$.Execution.Input.minimumGrade\",\"StringLessThanPath\":\"$.grade\",\"Next\":\"Notify\"}],\"Default\":\"Pass\"},\"Pass\":{\"Type\":\"Pass\",\"End\":true},\"Notify\":{\"End\":true,\"Type\":\"Task\",\"Resource\":\"arn:",
              {
                "Ref": "AWS::Partition"
              },
              ":states:::sns:publish\",\"Parameters\":{\"TopicArn\":\"",
              {
                "Ref": "TopicBFC7AF6E"
              },
              "\",\"Message.$\":\"States.JsonToString($)\"}},\"Fail\":{\"Type\":\"Fail\"}},\"TimeoutSeconds\":1800}"
            ]
          ]
        }
